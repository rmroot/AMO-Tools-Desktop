{
  "name": "amo-tools-desktop",
  "version": "0.2.1",
  "main": "main.js",
  "license": "MIT",
  "description": "AMO-Tools-Desktop",
  "author": "Advanced Manufacturing Office",
  "repository": "https://github.com/ORNL-AMO/AMO-Tools-Desktop.git",
  "angular-cli": {},
  "engines": {
    "node": "^8.1.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=4200",
    "build": "webpack",
    "build-prod": "ng build --prod --aot --env=prod",
    "clean": "rm -rf node_modules && rm -rf dist && rm -f package-lock.json",
    "electron": "electron .",
    "lint": "ng lint",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "pack": "build --dir",
    "dist": "build",
    "postinstall": "electron-builder install-app-deps",
    "windows": "./node_modules/.bin/build -w --x64 --ia32",
    "linux": "./node_modules/.bin/build -l --x64",
    "mac": "./node_modules/.bin/build -m --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.1",
    "@angular/common": "^4.2.1",
    "@angular/compiler": "^4.2.1",
    "@angular/compiler-cli": "^4.2.1",
    "@angular/core": "^4.2.1",
    "@angular/forms": "^4.2.1",
    "@angular/http": "^4.2.1",
    "@angular/platform-browser": "^4.2.1",
    "@angular/platform-browser-dynamic": "^4.2.1",
    "@angular/platform-server": "^4.2.1",
    "@angular/router": "^4.2.1",
    "ajv": "^5.2.2",
    "amo-tools-suite": "^0.2.2",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "d3": "4.5.0",
    "dotenv": "^4.0.0",
    "electron-log": "^1.3.0",
    "electron-updater": "^2.8.9",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "ngx-bootstrap": "^1.9.3",
    "ngx-electron": "1.0.2",
    "popper.js": "^1.12.3",
    "regression": "^2.0.0",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.1",
    "typescript": "^2.5.1",
    "zone.js": "^0.8.12"
  },
  "build": {
    "appId": "com.electron.amo-tools-desktop",
    "copyright": "Copyright 2017 ORNL. All rights reserved.",
    "productName": "AMO-Tools-Desktop",
    "directories": {
      "output": "../output/"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icons/win/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "installerHeaderIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "target": [
        "tar.gz",
        "deb"
      ],
      "icon": "src/assets/icons/png/",
      "executableName": "AMO-Tools-Desktop",
      "maintainer": "Advanced Manufacturing Office"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "src/assets/icons/mac/icon.icns"
    }
  },
  "devDependencies": {
    "@angular/cli": "^1.4.1",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.47",
    "autoprefixer": "^6.5.3",
    "chart.js": "^2.7.0",
    "circular-dependency-plugin": "^3.0.0",
    "codelyzer": "~2.0.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.35.1",
    "electron-packager": "^9.0.0",
    "electron-updater": "^2.15.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json2csv": "^3.7.3",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-loader": "^4.0.5",
    "lodash.foreach": "^4.5.0",
    "lodash.keys": "^4.2.0",
    "moment": "^2.18.1",
    "ng2-charts": "^1.5.0",
    "ng2-toasty": "^4.0.2",
    "node-loader": "^0.6.0",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^5.1.2",
    "protractor": "~5.1.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "^3.3.0",
    "tslint": "~4.4.2",
    "url-loader": "^0.6.2",
    "webpack": "~3.7.1",
    "webpack-concat-plugin": "1.4.0",
    "webpack-dev-server": "~2.7.1"
  }
}
